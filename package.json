  "license": "ISC",
  "dependencies": {
    "@asmtechno/iqlib": "^1.0.5",
    "@asmtechno/service-lib": "^1.0.14",
    "auto-version-js": "^0.3.10",
    "dotenv": "^16.4.5",
    "express-validator": "^7.1.0",
    "compression": "^1.7.4",
    "helmet": "^4.6.0",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^5.0.1",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "axios": "1.6.2",
    "jest": "29.7.0",
    "ts-jest": "^29.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@types/express": "^4.17.21",
