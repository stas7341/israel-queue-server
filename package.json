{
  "name": "IsraelQueueServer",
  "version": "1.0.1",
  "description": "The Israel Queue Server",
  "scripts": {
    "start-ts": "PROJECT_ENV=local ts-node ./src/app.ts",
    "start": "PROJECT_ENV=local node ./dist/app.js",
    "debug": "tsc; NODE_ENV=debug DEBUG=* node dist/app.js --unhandled-rejections=strict",
    "build": "tsc --alwaysStrict ",
    "load-modules": "npm i --also-dev",
