                      "type": "string"
                    },
                    "publisherQueue": {
                      "type": "string"
                    },
                    "subscriberUID": {
                      "type": "string"
                    },
                    "ts": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
