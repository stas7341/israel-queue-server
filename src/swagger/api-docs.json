          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Operation successful",
            "schema": {
              "type": "object",
              "properties": {
                "publisherQueue dynamic key": {
                  "type": "object",
                  "properties": {
                    "notificationQueue": {
                      "type": "string"
                    },
                    "groupCriteria": {
                      "type": "array",
